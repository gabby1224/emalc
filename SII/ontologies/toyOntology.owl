@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.w3.org/2002/07/owl#> .

[ rdf:type owl:Ontology
 ] .

#################################################################
#    Object Properties
#################################################################

###  #eat
<#eat> rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf owl:topObjectProperty .


###  #teach
<#teach> rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty .


###  http://www.w3.org/2002/07/owl#has
owl:has rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf owl:topObjectProperty .


###  http://www.w3.org/2002/07/owl#isPartOf
owl:isPartOf rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty .


#################################################################
#    Classes
#################################################################

###  #class
<#class> rdf:type owl:Class .


###  #grass
<#grass> rdf:type owl:Class .


###  #human
<#human> rdf:type owl:Class ;
         rdfs:subClassOf [ rdf:type owl:Restriction ;
                           owl:onProperty <#eat> ;
                           owl:someValuesFrom <#grass>
                         ] .


###  #teacher
<#teacher> rdf:type owl:Class ;
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty <#teach> ;
                             owl:allValuesFrom <#class>
                           ] .


###  http://www.w3.org/2002/07/owl#TV
owl:TV rdf:type owl:Class .


###  http://www.w3.org/2002/07/owl#child
owl:child rdf:type owl:Class .


###  http://www.w3.org/2002/07/owl#parent
owl:parent rdf:type owl:Class .


###  http://www.w3.org/2002/07/owl#screen
owl:screen rdf:type owl:Class .


#################################################################
#    General axioms
#################################################################

[ rdf:type owl:Restriction ;
  owl:onProperty owl:isPartOf ;
  owl:someValuesFrom owl:TV ;
  rdfs:subClassOf owl:screen
] .


[ rdf:type owl:Restriction ;
  owl:onProperty owl:has ;
  owl:allValuesFrom owl:parent ;
  rdfs:subClassOf owl:child
] .


